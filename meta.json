{
  "module_id": "ncs:viamrtsp",
  "visibility": "private",
  "url": "https://github.com/nicksanford/viamrtsp",
  "description": "module for rtsp, currently supports x264 and x265",
  "models": [
    {
      "api": "rdk:component:camera",
      "model": "ncs:viamrtsp:rtsp"
    },
    {
      "api": "rdk:component:camera",
      "model": "ncs:viamrtsp:rtsp-h264"
    },
    {
      "api": "rdk:component:camera",
      "model": "ncs:viamrtsp:rtsp-h265"
    },
    {
      "api": "rdk:component:camera",
      "model": "ncs:viamrtsp:rtsp-mjpeg"
    }
  ],
  "build": {
    "build": "bash build.sh",
    "path": "module.tar.gz",
    "arch" : ["linux/amd64", "linux/arm64"]
  },
  "entrypoint": "bin/viamrtsp"
}
